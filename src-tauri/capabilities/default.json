{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "sql:allow-close",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "core:window:allow-close",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-watch",
    "fs:allow-unwatch",
    "fs:allow-write-file",
    "fs:allow-appdata-read-recursive",
    "fs:allow-app-write-recursive",
    "dialog:default",
    "dialog:allow-open"
  ]
  
   
  
  
}